  "scripts": {
    "prebuild": "npm run lint",
    "build": "next build",
    "postbuild": "next-sitemap",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "build:migrate": "npm run db:migrate && next build",
    "types:check": "tsc --noEmit --pretty",
    "clean": "rimraf .next .swc out coverage",
    "commit": "cz",
    "deploy": "vercel",
    "dev": "next dev --port 3000",
    "dev:p": "run-p dev:*",
    "dev:spotlight": "spotlight-sidecar",
    "format": "prettier '**/*.{js,jsx,ts,tsx,json,yaml}' --write --ignore-path .gitignore --cache",
    "format:check": "prettier '**/*.{js,jsx,ts,tsx,json,yaml}' --check --ignore-path .gitignore --cache",
    "lint": "eslint .",
    "lint:debug": "eslint . --debug",
    "lint:fix": "eslint . --fix",
    "prepare": "[ -d .git ] && npx husky || echo 'ignore husky'",
    "start": "next start",
    "prisma:setup": "npx prisma generate && npx prisma db push && npx prisma db seed",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:serverless": "npx prisma generate --no-engine",
    "prisma:migrate": "npx prisma migrate dev && npx prisma migrate deploy",
    "prisma:push": "npx prisma db push",
    "prisma:seed": "npx prisma db seed",
    "prisma:studio": "npx prisma studio"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
